        button(id="btnB", class="button button--isi", style="width: 260px; cursor: pointer") Excel File To JSON String
    tr 
      td.t1(colspan="2", width="960", height="480")
        iframe(name="frameExcelChart", id="frameExcelChart", width="100%" height="100%")